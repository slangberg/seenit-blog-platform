<% include header %>
<body>
	<h1>Sign In</h1>
	<signin>
		<form>
			<div>{{errormsg}}</div>
			<div><input type="text" ng-model="user.name"  placeholder="Reddit Username"/></div>
			<div><input type="password" ng-model="user.pass"  placeholder="Reddit Password"/></div>
			<input type="submit" ng-click="submitUserName(user)" value="Login" />
		</form>
	</signin>
</body>
</html>