include header.jade
  body
    .page
      header
        h1 Reddi Blog 
      blogroll
        ul.blog-role
          li.blog-item(ng-repeat='post in postsData')
            .blog-img-holder
              a.blog-img-link
                img.blog-img(ng-src='{{post.imgurl}}')
            .blog-info-holder
              a.post-tile(ng-href='{{post.permalink}}')
                h2 {{post.title}}
